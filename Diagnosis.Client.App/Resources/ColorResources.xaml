<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="mainWindowBackgroundBrush" Color="{DynamicResource {x:Static SystemColors.WindowColorKey}}" />
    <SolidColorBrush x:Key="mainMenuBackgroundBrush" Color="{DynamicResource {x:Static SystemColors.ControlColorKey}}" />
    <SolidColorBrush x:Key="overlayBackgroundBrush"  Color="{DynamicResource {x:Static SystemColors.ControlColorKey}}" />    
    <SolidColorBrush x:Key="hrEditorBackgroundBrush" Color="{DynamicResource {x:Static SystemColors.ControlColorKey}}" />
    
    <SolidColorBrush x:Key="searchHighlightBackgroundBrush" Color="Yellow" />
    <SolidColorBrush x:Key="searchHighlightForegroundBrush" Color="Black" />

    <SolidColorBrush x:Key="treeItemSelectedBrush"
                     Color="#FFFF8A16" />
    
    <SolidColorBrush x:Key="newWordBrush"
                     Color="#FFdaffa5" />
    <SolidColorBrush x:Key="forbiddenTagBrush"
                     Color="#FFffa5a5" />
    <SolidColorBrush x:Key="partialMeasureBrush"
                     Color="#FFfffaa5" />
</ResourceDictionary>